@article{squicciarini2015HumanCapitalIndustrialization,
  title = {Human {{Capital}} and {{Industrialization}}: {{Evidence}} from the {{Age}} of {{Enlightenment}} *},
  shorttitle = {Human {{Capital}} and {{Industrialization}}},
  author = {Squicciarini, Mara P. and Voigtl{\"a}nder, Nico},
  year = {2015},
