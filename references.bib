@article{taalbi2019InnovationWavesTechnological,
  title = {Innovation Waves and Technological Transitions: {{Sweden}}, 1909-2016},
  shorttitle = {Innovation Waves and Technological Transitions},
  author = {Taalbi, Josef},
  year = {2019},
  journal = {Innovation waves and technological transitions: Sweden, 1909-2016},
  series = {Lund {{Papers}} in {{Economic History}}. {{General Issues}}},
  abstract = {There are important unresolved questions about long-term trends of in- novation activity and the nature of the interplay between innovation  and economic development and transformation. This study explores the promise of a literature-based innovation output indicator, constructed for the Swedish engineering industry, 1909-2016. The findings suggest a long-run increasing trend in innovations per capita. Meanwhile, product innovations have also become more complex and it is suggested that crude innovation counts underestimate the long-run innovation performance. In order to analyse innovation and economic development across different frequencies, the study uses a wavelet decomposition approach. The results suggest that innovation activity has surged in periods of intense industry rationalization and struc- tural crisis (1930s, 1970s and 2010s) and that such pulses were intimately connected to the second and third industrial revolutions.},
  keywords = {Innovation,N13,O14,O31,Technological systems,Wavelet analysis}
}

@article{taalbi2021InnovationLongRun,
  title = {Innovation in the Long Run: {{Perspectives}} on Technological Transitions in {{Sweden}} 1908--2016},
  shorttitle = {Innovation in the Long Run},
  author = {Taalbi, Josef},
  year = {2021},
  month = sep,
  journal = {Environmental Innovation and Societal Transitions},
  volume = {40},
  pages = {222--248},
  issn = {2210-4224},
  doi = {10.1016/j.eist.2021.07.003},
  urldate = {2025-04-07},
  abstract = {There are fundamental unresolved questions about the nature of the long-run interplay between innovation and economic development processes. This study examines technological transitions in Sweden, 1908--2016, using a new literature-based innovation output indicator for the engineering industry, allowing a mixed-methods approach. The results suggest that waves of innovation have taken place during periods of investment downturns and structural crises during the 1930s, 1970s, and 2010s. Closer examination indicates that landscape pressure alone cannot explain the pattern. Rather, the pattern can be explained by multi-regime interactions: infrastructure and industry rationalization, acting as a fertile ground for breakthrough innovations when the crisis breaks out. The results support a historical interpretation of long-run transitions in terms of development blocks and three industrial revolutions, with implications for the assessment of current sustainable transitions. This suggests a crucial role for infrastructures and interactions between socio-technical systems, not least between digital and renewable energy technology.},
  keywords = {Development blocks,Innovation,Technological transitions,Wavelet analysis}
}

