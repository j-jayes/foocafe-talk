}

@article{rossi2024ProblemsLLMgeneratedData,
  title = {The {{Problems}} of {{LLM-generated Data}} in {{Social Science Research}}},
  author = {Rossi, Luca and Harrison, Katherine and Shklovski, Irina},
  year = {2024},
