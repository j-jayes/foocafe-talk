@article{schaefer2014LongtermGrowthDriven,
  title = {Long-Term Growth Driven by a Sequence of General Purpose Technologies},
  author = {Schaefer, Andreas and Schiess, Daniel and Wehrli, Roger},
  year = {2014},
  journal = {Economic Modelling},
  volume = {37},
  number = {C},
  pages = {23--31},
  publisher = {Elsevier},
  urldate = {2025-04-07},
  abstract = {We present a Schumpterian model of endogenous growth with General Purpose Technologies (GPTs) that captures two important historical stylized facts: First, from the beginning of mankind until today GPTs are arriving at an increasing frequency and, second, all GPTs heavily depended on previous technologies. In our model, the arrival of GPTs is endogenous and arises stochastically depending on the currently available applied knowledge stock. This way of endogenizing the arrival of new GPTs allows for a model which is more in tune with the historical reality than the existing GPT models.},
  langid = {english},
  keywords = {General Purpose Technologies,Research and development,Schumpeterian growth}
}

